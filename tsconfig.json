{
  "extends": "@meslzy/tsconfig",
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noUnusedParameters": false,
    "declaration": true,
    "rootDirs": [
      "src",
      "styled"
    ],
    "outDir": "dist",
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "~styled/*": [
        "./styled/*"
      ]
    }
  },
  "include": [
    "src",
    "styled"
  ],
  "exclude": [
    "dist"
  ]
}
